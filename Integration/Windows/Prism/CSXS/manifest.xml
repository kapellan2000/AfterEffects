<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest
	Version="5.0"
	ExtensionBundleId="com.extension.prism"
	ExtensionBundleVersion="1.0.0"
	ExtensionBundleName="Prism"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<ExtensionList>
		<Extension Id="com.extension.prism.panel" Version="1.0" />
	</ExtensionList>

	<ExecutionEnvironment>
		<HostList>
			<Host Name="AEFT" Version="12.0" />
		</HostList>

	<LocaleList>
		<Locale Code="All" />
	</LocaleList>

	<RequiredRuntimeList>
		<RequiredRuntime Name="CSXS" Version="5.0" />
	</RequiredRuntimeList>
</ExecutionEnvironment>

	<DispatchInfoList>
		<Extension Id="com.extension.prism.panel">
			<DispatchInfo>

			<Resources>
				<MainPath>./index.html</MainPath>
				<CEFCommandLine>
				<Parameter>--enable-nodejs</Parameter>
				<Parameter>--mixed-context</Parameter>
				</CEFCommandLine>
			</Resources>

			<Lifecycle>
				<AutoVisible>true</AutoVisible>
			</Lifecycle>

			<UI>

				<Type>Panel</Type>
				<Menu>Prism</Menu>

				<Geometry>
					<Size>
						<Height>580</Height>
						<Width>400</Width>
					</Size>
				
					
				</Geometry>

			</UI>
		</DispatchInfo>
	</Extension>
</DispatchInfoList>
</ExtensionManifest>
